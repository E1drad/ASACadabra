<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="ASACadabra" nsURI="http://www.example.org/ASACadabra" nsPrefix="ASACadabra">
  <eClassifiers xsi:type="ecore:EClass" name="Composant">
    <eStructuralFeatures xsi:type="ecore:EReference" name="interfaceFournie" upperBound="-1"
        eType="#//InterfaceComposant" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="interfaceRequise" upperBound="-1"
        eType="#//InterfaceComposant" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Connecteur">
    <eOperations name="Glue">
      <eParameters name="roleFournis" eType="#//RoleConnecteur"/>
      <eParameters name="roleRequis" eType="#//RoleConnecteur"/>
    </eOperations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="roleFournis" upperBound="-1"
        eType="#//RoleConnecteur" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="roleRequis" upperBound="-1"
        eType="#//RoleConnecteur" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Configuration">
    <eStructuralFeatures xsi:type="ecore:EReference" name="composant" lowerBound="1"
        upperBound="-1" eType="#//Composant" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="connecteur" upperBound="-1"
        eType="#//Connecteur" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="interfaceconfiguration"
        upperBound="-1" eType="#//InterfaceConfiguration" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PortComposant"/>
  <eClassifiers xsi:type="ecore:EClass" name="ServicesComposant"/>
  <eClassifiers xsi:type="ecore:EClass" name="InterfaceComposant">
    <eStructuralFeatures xsi:type="ecore:EReference" name="servicesRequis" upperBound="-1"
        eType="#//ServicesComposant" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="servicesFournis" upperBound="-1"
        eType="#//ServicesComposant" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="portRequis" upperBound="-1"
        eType="#//PortComposant" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="portFournis" upperBound="-1"
        eType="#//PortComposant" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="RoleConnecteur"/>
  <eClassifiers xsi:type="ecore:EClass" name="LienAttachement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="interface" lowerBound="1"
        eType="#//PortComposant" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="role" lowerBound="1" eType="#//RoleConnecteur"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="LienBinding">
    <eStructuralFeatures xsi:type="ecore:EReference" name="port" lowerBound="1" eType="#//PortComposant"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="portconfiguration" lowerBound="1"
        eType="#//PortConfiguration" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PortConfiguration"/>
  <eClassifiers xsi:type="ecore:EClass" name="InterfaceConfiguration">
    <eStructuralFeatures xsi:type="ecore:EReference" name="portconfiguration" upperBound="-1"
        eType="#//PortConfiguration" containment="true"/>
  </eClassifiers>
</ecore:EPackage>
